
  
#include <stdio.h>
#include <math.h>

int main() {
    double X1, Y1, X2, Y2;
    double distance;

    // 读取输入的坐标
    scanf("(%lf,%lf),(%lf,%lf)", &X1, &Y1, &X2, &Y2);

    // 计算两点之间的距离
    distance = sqrt((X2 - X1) * (X2 - X1) + (Y2 - Y1) * (Y2 - Y1));

    // 输出结果，保留到小数点后三位
    printf("%.3lf\n", distance);

    return 0;
}
 
